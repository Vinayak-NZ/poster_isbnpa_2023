	pmid = {27662265},
	keywords = {Female, Humans, Male, Aged, Physician-Patient Relations, Communication, Focus Groups, Health Literacy, Health Services Accessibility, Health Services Needs and Demand, Narration, Patient Preference, Photography, Primary Health Care, Role Playing},
