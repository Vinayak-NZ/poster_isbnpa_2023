	author = {Clarke, Jenelle and Waring, Justin and Timmons, Stephen},
	year = {2019},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/spol.12459},
	keywords = {coproduction of knowledge, inclusivity, interaction rituals, situated practice, sustainability, translational research},
	pages = {233--248},
	file = {Full Text PDF:/Users/vinayakanand-kumar/Zotero/storage/UDF2RZX5/Clarke et al. - 2019 - The challenge of inclusive coproduction The impor.pdf:application/pdf;Snapshot:/Users/vinayakanand-kumar/Zotero/storage/W8GTDKVP/spol.html:text/html},
}

