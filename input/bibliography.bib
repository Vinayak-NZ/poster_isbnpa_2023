	keywords = {Female, Humans, Rehabilitation, Exercise, Male, Adaptation, Psychological, Age Factors, Aged, Aged, 80 and over, Germany, Health Behavior, Health Resources, Outcome and Process Assessment, Health Care},
	pages = {500--518},
}

@article{ziegelmann_planning_2007,
	title = {Planning and strategy use in health behavior change: a life span view},
	volume = {14},
	issn = {1070-5503},
	shorttitle = {Planning and strategy use in health behavior change},
	doi = {10.1007/BF02999225},
	abstract = {Exercise-related strategy use and planning were investigated in younger (18-49 years) and older individuals (50-80 years) in orthopedic rehabilitation who were supposed to adhere to a strict exercise regimen. As part of a longitudinal study, N=368 individuals completed questionnaires assessing the amount of physical activities performed pre-rehabilitation and 6 and 12 months after discharge. In addition, the extent of strategy use (selection, optimization, and compensation) and planning after discharge was assessed. Planning was subdivided into two constructs: action planning (planning when, where, and how to exercise) and coping planning (planning how to exercise in the face of barriers). Two-sample structural equation modeling was used. For both age groups, strategy use improved prediction of exercise goal attainment on top of planning, while strategy use mediated the relationship between coping planning and goal attainment. Interventions fostering strategy use as well as planning might enhance exercise adherence across age groups.},
	language = {eng},
	number = {1},
	journal = {International Journal of Behavioral Medicine},
	author = {Ziegelmann, Jochen P. and Lippke, Sonia},
